 <option value="mus:j">Musiikkiäänite</option>
 <option value="mus:i">Puheäänite</option>
 </select>
 </fieldset>
 </div> <!-- /.advsearch_limit -->
 </div>

 <div class="col-sm-6 col-lg-3">
 <div class="advsearch_limit">
 <fieldset>
 <label for="subtype_format">Materiaali</label>
 <select id="subtype_format" name="limit" class="subtype form-select">
 <option value="" selected="selected" class="menuheader">Materiaali</option>
 <option value="l-format:ta">Tekstiaineisto</option>
 <option value="l-format:tb">Isoteksti</option>
 <option value="l-format:tc or l-format:fb">Pistekirjoitus</option>
