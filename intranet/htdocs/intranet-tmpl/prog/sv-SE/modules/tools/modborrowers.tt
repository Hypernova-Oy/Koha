 [% IF ( op == 'show_form' ) %]
 <h1>Satsvis redigering av låntagare</h1>
 <form id="patron_batchmod_form" method="post" enctype="multipart/form-data" action="/cgi-bin/koha/tools/modborrowers.pl">
 [% INCLUDE 'csrf-token.inc' %]
 <input type="hidden" name="op" value="cud-show" />

 [% WRAPPER tabs id= "batch_patron_options" %]
 [% WRAPPER tabs_nav %]
 [% WRAPPER tab_item tabname= "usecardnumber" bt_active= 1 %] <span>Av kortnummer</span> [% END %]
 [% WRAPPER tab_item tabname= "useborrowernumber" %] <span>Av låntagarnummer</span> [% END %]
 [% IF patron_lists %]
 [% WRAPPER tab_item tabname= "uselist" %] <span>Av låntagarlista</span> [% END %]
 [% END %]
 [% END # /WRAPPER tabs_nav %]

 [% WRAPPER tab_panels %]
 [% WRAPPER tab_panel tabname="usecardnumber" bt_active= 1 %]
 <fieldset class="rows">
 <legend>Använd en fil med kortnummer</legend>
 <ol>
 <li>
 <label for="cardnumberuploadfile">Fil: </label> <input type="file"
                                                    id="cardnumberuploadfile" name="cardnumberuploadfile" />
 <div class="hint">Filen måste innehålla ett kortnummer per rad.</div>
 </li>
 </ol>
 </fieldset>
 <fieldset class="rows">
 <legend>Eller lista kortnummer ett efter ett</legend>
 <ol>
 <li>
 <label for="cardnumberlist">Kortnummerlista (ett kortnummer per rad): </label>
 <textarea rows="10" cols="30" id="cardnumberlist"
                                                    name="cardnumberlist">[% cardnumberlist | html %]</textarea>
 </li>
 </ol>
 </fieldset>
 <fieldset class="action">
 <input class="btn btn-primary" type="submit" value="Fortsätt" />
 <a class="cancel" href="/cgi-bin/koha/tools/tools-home.pl">Avbryt</a>
 </fieldset>
 [% END # /tab_panel# %]

 [% WRAPPER tab_panel tabname="useborrowernumber" %]
 <fieldset class="rows">
 <legend>Använd en fil med låntagarnummer</legend>
 <ol>
 <li>
 <label for="borrowernumberuploadfile">Fil: </label> <input type="file"
                                                    id="borrowernumberuploadfile" name="borrowernumberuploadfile" />
 <div class="hint">Ett låntagarnummer per rad.</div>
 </li>
 </ol>
 </fieldset>
 <fieldset class="rows">
 <legend>Lista låntagarnummer ett och ett</legend>
 <ol>
 <li>
 <label for="borrowernumberlist">Låntagarnummerlista (ett kortnummer per rad): </label>
 <textarea rows="10" cols="30" id="borrowernumberlist"
                                                    name="borrowernumberlist">[% borrowernumberlist | html %]</textarea>
 </li>
 </ol>
 </fieldset>
 <fieldset class="action">
 <input class="btn btn-primary" type="submit" value="Fortsätt" />
 <a class="cancel" href="/cgi-bin/koha/tools/tools-home.pl">Avbryt</a>
 </fieldset>
 [% END # /tab_panel# %]

 [% IF patron_lists %]
 [% WRAPPER tab_panel tabname="uselist" %]
 <fieldset class="rows">
 <legend>Använd en låntagarlista</legend>
 <ol>
 <li>
 <label for="patron_list_id">Låntagarlista: </label>
 <select id="patron_list_id" name="patron_list_id">
 <option value=""> -- Välj en låntagarlista -- </option>
 [% FOREACH pl IN patron_lists %]
 <option value="[% pl.patron_list_id | html %]">[% pl.name | html %]</option>
 [% END %]
 </select>
 </li>
 </ol>
 </fieldset>
 <fieldset class="action">
 <input class="btn btn-primary" type="submit" value="Fortsätt" />
 <a class="cancel" href="/cgi-bin/koha/tools/tools-home.pl">Avbryt</a>
 </fieldset>
 [% END # /tab_panel# %]
 [% END %]
 [% END # /WRAPPER tab_panels %]
 [% END # /WRAPPER tabs %]
 </form>
 [% END %]

 [% IF ( op == 'show') && (!borrowers) && (!notfoundcardnumbers) # Alert if no patrons given%]
 [% op = 'noshow' # Change op to prevent display in code below %]
 <h1>Satsvis redigering av låntagare</h1>
 <div class="alert alert-warning">
 <p>Inga kortnummer eller låntagarnumer angavs.</p>
 <form action="/cgi-bin/koha/tools/modborrowers.pl" method="get">
 <button type="submit" class="btn btn-default approve"><i class="fa fa-fw fa-check"></i> OK</button>
 </form>
 </div>
 [% END #Alert if no patrons %]

 [% IF ( op == 'show' or op == 'show_results' ) %]
