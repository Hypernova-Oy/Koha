 [% END %]
 [% END %]

 [% IF ( add_form ) %]
 [% WRAPPER breadcrumb_item bc_active= 1 %]
 [% IF ( modify ) %]
 [% tx("Modify system preference '{system_preference}'", { system_preference = searchfield }) | html %]
