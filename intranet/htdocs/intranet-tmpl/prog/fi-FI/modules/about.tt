[% USE raw %]
[% USE HtmlTags %]
[% USE Koha %]
[% USE Asset %]
[% USE KohaDates %]
[% PROCESS 'i18n.inc' %]
[% SET footerjs = 1 %]
[% INCLUDE 'doc-head-open.inc' %]
<title>[% FILTER collapse %]
 [% t("About Koha") | html %] &rsaquo;
 [% t("Koha") | html %]
[% END %]</title>
[% INCLUDE 'doc-head-close.inc' %]
</head>
<body id="about_about" class="about">
[% INCLUDE 'header.inc' %]
[% PROCESS 'about-team.inc' %]

[% WRAPPER 'sub-header.inc' %]
 [% WRAPPER breadcrumbs %]
 [% WRAPPER breadcrumb_item %]
 <a href="#" aria-current="page">Tietoja Kohasta</a>
 [% END %]
 [% END %]
[% END %]

<div class="main container-fluid">
 <div class="row">
 <div class="col-md-12">
 [% INCLUDE 'messages.inc' %]

 <h1>Tietoja Kohasta</h1>

 [% WRAPPER tabs id= "abouttabs" %]
 [% WRAPPER tabs_nav %]
 [% WRAPPER tab_item linktab = 1 tabname = "about" bt_active = tab == "about" %] <span>Palvelimen tiedot</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "perl" bt_active = tab == "perl" %] <span>Perl-moduulit</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "sysinfo" bt_active = tab == "sysinfo" %] <span>Järjestelmätiedot</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "database" bt_active = tab == "database" %] <span>Database audit</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "team" bt_active = tab == "team" %] <span>Koha-tiimi</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "licenses" bt_active = tab == "licenses" %] <span>Lisenssit</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "translations" bt_active = tab == "translations" %] <span>Käännökset</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "history" bt_active = tab == "history" %] <span>Aikajana</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "dedications" bt_active = tab == "dedications" %] <span>Omistuskirjoitukset</span> [% END %]
 [% END %]
 [% WRAPPER tab_panels %]
 [% SWITCH tab %]
 [% CASE 'about' %] [% PROCESS about_panel %]
 [% CASE 'perl' %] [% PROCESS perl_panel %]
 [% CASE 'sysinfo' %] [% PROCESS sysinfo_panel %]
 [% CASE 'database' %] [% PROCESS database_panel %]
 [% CASE 'team' %] [% PROCESS team_panel %]
 [% CASE 'licenses' %] [% PROCESS licenses_panel %]
 [% CASE 'translations' %] [% PROCESS translations_panel %]
 [% CASE 'history' %] [% PROCESS history_panel %]
 [% CASE 'dedications' %] [% PROCESS dedications_panel %]
 [% CASE %] [% PROCESS about_panel %]
 [% END %]
 [% END %]
 [% END # /WRAPPER abouttabs %]
 <div id="koha_community"><a href="https://www.koha-community.org"><i class="fa-solid fa-globe"></i> Koha-yhteisö</a></div>
 </div> <!-- /.col-md-12 -->
 </div> <!-- /.row -->
<!-- the main div is closed in intranet-bottom.inc -->
[% INCLUDE 'intranet-bottom.inc' %]

[% BLOCK about_panel %]
 [% WRAPPER tab_panel tabname= "about" bt_active = 1 %]
 <table>
 <caption>Palvelimen tiedot</caption>
 <tr>
 <th scope="row">Kohan versio: </th>
 <td>[% kohaVersion | html %][% IF kohaCodename %] <strong>[% kohaCodename | html %]</strong>[% END %]</td>
 </tr>
 <tr>
 <th scope="row">Käyttöjärjestelmä ('uname -a'): </th>
 <td>[% osVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">Perl interpreter: </th>
 <td>[% perlPath | html %]</td>
 </tr>
 <tr>
 <th scope="row">Perl-versio: </th>
 <td>[% perlVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">Perl @INC: </th>
 <td>
 [% FOREACH perlIncPat IN perlIncPath %]
 [% perlIncPat.perlinc | html %] <br />
 [% END %]
 </td>
 </tr>
 <tr>
 <th scope="row">MySQL versio: </th>
 <td>[% mysqlVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">Apache-versio: </th>
 <td>[% apacheVersion | html %]</td>
 </tr>
 [% IF (is_psgi) %]
 <tr>
 <th scope="row">PSGI: </th>
 <td>[% psgi_server | html %]</td>
 </tr>
 [% END %]
 [% IF Koha.Preference('SearchEngine') == 'Elasticsearch' %]
 <tr>
 <th scope="row">Elasticsearch: </th>
 [% IF elasticsearch_fatal_config_error %]
 <td><span class="text-bg-warning">[% elasticsearch_fatal_config_error | html %]</span></td>
 [% ELSE %]
 [% IF elasticsearch_status.running %]
 <td class="text-bg-info">
 [% ELSE %]
 <td>
 [% END %]
 <span>Versio:</span>
 <span>[% elasticsearch_status.version | html %]</span>
 |
 <span>Solmut:</span>
 <span>[% elasticsearch_status.nodes.join(' / ') | html %]</span>
 |
 <span>Tila:</span>
 [% IF elasticsearch_status.running %]
 <span>toiminnassa</span>
 |
 <span>Indeksit:</span>
 [% FOREACH index IN elasticsearch_status.indexes %]
 [% index.index_name | html %]
 (<span>määrä:</span> <em>[% index.index_count | html %]</em>)
 [% UNLESS loop.last %], [% END %]
 [% END %]
 [% ELSE %]
 <span class="text-bg-warning">ei toiminnassa</span>
 [% END %]
 </td>
 [% END # /IF elasticsearch_fatal_config_error %]
 </tr>
 [% END %]
 <tr>
 <th scope="row">Memcached: </th>
 [% IF memcached_running AND is_memcached_still_active AND where_is_memcached_config == 'config_only' %]
 <td class="text-bg-info">
 [% ELSE %]
 <td>
 [% END %]
 <span>Palvelimet:</span>
 [% IF memcached_servers %]
 <span>[% memcached_servers | html %]</span>
 [% ELSE %]
 <span>määrittämätön</span>
 [% END %] |
 <span>Nimiavaruus:</span>
 [% IF memcached_namespace %]
 <span>[% memcached_namespace | html %]</span>
 [% ELSE %]
 <span>määrittämätön</span>
 [% END %] |
 <span>Tila:</span>
 [% IF memcached_servers %]
 [% IF memcached_running and is_memcached_still_active %]
 <span class="text-bg-info">toiminnassa</span>.
 [% ELSE %]
 <span class="text-bg-warning">ei toiminnassa</span>.
 [% IF is_psgi %]
 <span>Muista, että memcached pitää käynnistää ennen Plackia.</span>
 [% END %]
 [% END %]
 [% ELSE %]
 <span>tuntematon</span>
 [% END %] |
 <span>Asetus luettu:</span>
 [% SWITCH where_is_memcached_config %]
 [% CASE 'config_only' %]
 <span class="text-bg-info">koha-conf.xml</span>
 [% CASE 'ENV_only' %]
 <span class="text-bg-warning">ENV</span> <span>Huomaa, että memcached määritellään $KOHA_CONF-tiedostossa</span>
 [% CASE 'both' %]
 <span class="text-bg-warning">ENV ja koha-conf.xml</span> <span>Huomaa, että memcached määritellään $KOHA_CONF-tiedostossa. Välttääksesi väärinymmärryksiä, älä tuo memcached-kokoonpanoa ENV:stä.</span>
 [% CASE # nowhere %]
 <span class="text-bg-warning">Ei missään</span> <span>Huomaa, että memcached määritellään $KOHA_CONF-tiedostossa. Tällä hetkellä ei ole määriteltynä kelvollista memcached-kokoonpanoa.</span>
 [% END %]
 [% IF effective_caching_method != 'Cache::Memcached::Fast::Safe' %]
 | <span>Voimassa oleva välimuistitapa:</span> [% effective_caching_method | html %]
 [% END %]
 </td>
 </tr>
 <tr>
 <th scope="row">Zebra-versio: </th>
 <td>[% zebraVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">Zebra-status: </th>
 [% IF (errZebraConnection == 10000) %]
 <td class="text-bg-danger"><strong>Zebra-palvelin ei ole saatavilla. Onko se käynnissä?</strong></td>
 [% ELSIF (errZebraConnection) %]
 <td class="text-bg-warning"><strong>Zebra-virheilmoitus: [% ( errZebraConnection ) | html %]</strong></td>
 [% ELSE %]
 <td class="text-bg-info">Toiminnassa</td>
 [% END %]
 </tr>
 <tr>
 <th scope="row">Viestin välittäjä: </th>
 [% IF warnConnectBroker == 'Error connecting' %]
 <td class="text-bg-warning"><strong>Käytetään SQL-kyselyä (Varasuunnitelma, Virhe yhdistettäessä RabbitMQ-palveluun)</strong></td>
 [% ELSIF warnConnectBroker %]
 <td class="text-bg-warning"><strong>Käytetään SQL-kyselyä</strong></td>
 [% ELSE %]
 <td class="text-bg-info">Käytetään RabbitMQ:ta</td>
 [% END %]
 </tr>
 <tr>
 <th scope="row">Päivämäärä ja aika: </th>
 <td>[% current_date_and_time | $KohaDates  with_hours => 1 %]</td>
 </tr>
 <tr>
 [% timezone_config_class = (time_zone.config_invalid) ? 'text-bg-warning' : '' %]
 [% timezone_env_class    = (time_zone.env_invalid)    ? 'text-bg-warning' : '' %]
 <th scope="row">Aikavyöhyke: </th>
 <td>
 <span>Käytetty:</span> <span>[% time_zone.actual | html %]</span>
 [% IF time_zone.actual_bad_tz_fallback %]
 <span>(Tämä on palautusarvo väärän kokoonpanon johdosta)</span>
 [% END %]
 |
 <span>Määrittely:</span>
 [% IF time_zone.config != '' %]
 <span class="[% timezone_config_class | html %]">[% time_zone.config | html %]</span>
 [% ELSE %]
 <span>Määrittelemätön</span>
 [% END %] |
 <span>Ympäristö (TZ):</span>
 [% IF time_zone.environment != '' %]
 <span class="[% timezone_env_class | html %]">[% time_zone.environment | html %]</span>
 [% ELSE %]
 <span>Määrittelemätön</span>
 [% END %]
 </td>
 </tr>
 </table>
 [% END # tab=about %]
[% END %]

[% BLOCK perl_panel %]
 [% WRAPPER tab_panel tabname= "perl" bt_active = 1 %]
 <table style="cursor:pointer">
 <caption>Perl-moduulit</caption>
 [% FOREACH tabl IN table %]
 <tr>
 [% FOREACH ro IN tabl.row %]
 [% IF ( ro.current ) %]
 [% IF ( ro.require ) %]
 <th scope="row" style="font-weight:bold;" title="Tämänhetkinen osio">
 [% ELSE %]
 <th scope="row" style="font-weight:normal" title="Tämänhetkinen osio">
 [% END %]
 [% ELSIF ( ro.upgrade ) %]
 [% IF ( ro.require ) %]
 <th scope="row" style="background-color:#FFCB2F;font-weight:bold;" title="Osion päivitys vaaditaan">
 [% ELSE %]
 <th scope="row" style="background-color:#FFCB2F;font-weight:normal" title="Osion päivitys vaaditaan">
 [% END %]
 [% ELSE %]
 [% IF ( ro.name == '' ) %]
 <th>
 [% ELSIF ( ro.require ) %]
 <th scope="row" style="background-color:#FF8A8A;font-weight:bold;" title="Vaadittu osio puuttuu">
 [% ELSE %]
 <th scope="row" style="background-color:#FF8A8A;font-weight:normal" title="Valinnainen osio puuttuu">
 [% END %]
 [% END # /IF ro.current %]
 [% IF ( ro.name ) %]
 [% ro.name | html %]
 <span style="font-weight:normal; font-size:smaller">
 ([% ro.reqversion | html %]
 [% IF ro.maxversion %] - [% ro.maxversion | html %][% END %]
 [% IF ro.excversion %][% FOR v IN ro.excversion %], ![% v | html %][% END %][% END %])
 </span>
 [% END %]
 </th>
 [% IF ( ro.name == '' ) %]
 <td>
 [% ELSIF ( ro.version ) %]
 <td>[% ro.version | html %]
 [% ELSE %]
 <td style="font-weight:bold">Ei asennettu [% END %] </td>
 [% END # /FOREACH ro %]
 </tr>
 [% END # /FOREACH tabl %]
 </table>
 [% END # tab=perl %]
[% END %]

[% BLOCK sysinfo_panel %]
 [% IF Asset.css("css/staff-global.css") == "" || Asset.js("js/vue/dist/erm.js") == "" %]
 [% SET warnMissingCompiledFiles = 1 %]
 [% END %]
 [% WRAPPER tab_panel tabname= "sysinfo" bt_active = 1 %]
 [% IF weasyprint_missing || warnPrefRequireChoosingExistingAuthority || warnPrefEasyAnalyticalRecords || warnPrefAnonymousPatronOPACPrivacy || warnPrefAnonymousPatronAnonSuggestions || warnPrefAnonymousPatronOPACPrivacy_PatronDoesNotExist || warnPrefAnonymousPatronAnonSuggestions_PatronDoesNotExist || warnPrefKohaAdminEmailAddress || warnPrefOpacHiddenItems || warnPrefPatronSelfRegistrationDefaultCategory || invalid_yesno.count || warnNoActiveCurrency || warnIsRootUser || xml_config_warnings.size || warnI18nMissing || AutoSelfCheckPatronDoesNotHaveSelfCheckPerm || AutoSelfCheckPatronHasTooManyPerm || warnStatisticsFieldsError || warnNoTemplateCaching || warnILLConfiguration || has_ai_issues || oauth2_missing_deps || bad_yaml_prefs || warnRelationships || log4perl_errors || config_bcrypt_settings_no_set || warnHiddenBiblionumbers.size || warnConnectBroker || elasticsearch_has_missing || warnMissingCompiledFiles || warnDbRowFormat %]
 [% IF ( warnDbRowFormat ) %]
 <h2>Tietokannan rivien formaatti väärä</h2>
 <p>Tietokannan taulut, joissa riviformaatti muuta kuin 'DYNAMIC': [% warnDbRowFormat | html %]</p>
 <p>Sinulle voi tulla ongelmia päivittäessäsi Kohaa uudempaan versioon, jos et päivitä tietokannan taulujen riviformaattia.</p>
 <p>Tietääksesi, kuinka välttää tämä ongelma, katso wikisivu: <a href="https://wiki.koha-community.org/wiki/Database_row_format">Tietokannan rivien formaatti</a>
 </p>
 [% END %]

 [% IF (warnIsRootUser) %]
 <h2>Varoitus koskien nykyistä käyttäjää</h2>
 <p>Olet kirjautuneena tietokannan hallinnan tunnuksella. Tämä ei ole suositeltavaa, koska jotkin Kohan ominaisuuksista eivät toimi oikein tällä tunnuksella.</p>
 <p>Kirjaudu sisään normaalilla virkailijatunnuksella.</p>
 [% END # /IF warnIsRootUser %]

 [% IF has_ai_issues %]
 <h2>Tietoon liittyviä ongelmia</h2>
 <p>
 Joissain tietokannan tauluissa on ongelmia auto_increment-arvojen kanssa, mikä saattaa johtaa tiedon häviämiseen. </p>
 <p>
 <strong>Älä jätä tätä varoitusta huomioimatta.</strong>
 </p>
 <p>
 Ongelma on, että InnoDB ei pidä auto_incrementiä tallessa, kun SQL-palvelin käynnistyy uudelleen (se on vain muistissa). Joten palvelimen käynnistyessä auto_increment-arvoiksi asetetaan max(table.id)+1. </p>
 <p>Tietääksesi, kuinka välttää tämä ongelma, katso wikisivu: <a href="https://wiki.koha-community.org/wiki/DBMS_auto_increment_fix">DBMS autoincrement -korjaus</a>
 </p>

 <h3>Löytyi ongelmia</h3>
 [% IF ai_patrons %]
 <h4>Asiakkaat</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "borrowers" | $HtmlTags tag="strong" %] ja [% "deletedborrowers" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR p IN ai_patrons %]
 [% p.borrowernumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_biblios %]
 <h4>Nimeketietueet</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "biblio" | $HtmlTags tag="strong" %] ja [% "deletedbiblio" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR b IN ai_biblios %]
 [% b.biblionumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_biblioitems %]
 <h4>Nimeketietueet</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "biblioitems" | $HtmlTags tag="strong" %] ja [% "deletedbiblioitems" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR b IN ai_biblioitems %]
 [% b.biblioitemnumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_items %]
 <h4>Niteet</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "items" | $HtmlTags tag="strong" %] ja [% "deleteditems" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR i IN ai_items %]
 [% i.itemnumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_checkouts %]
 <h4>Lainat</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "issues" | $HtmlTags tag="strong" %] ja [% "old_issues" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR c IN ai_checkouts %]
 [% c.issue_id | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_holds %]
 <h4>Varaukset</h4>
 <p>Seuraavat ID:t ovat kummassakin taulussa [% "reserves" | $HtmlTags tag="strong" %] ja [% "old_reserves" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR h IN ai_holds %]
 [% h.reserve_id | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 <br/>
 [% END # /IF has_ai_issues %]

 [% IF warnRelationships %]
 <h2>Ongelmat asiakkaiden takaajatietomäärityksissä</h2>
 [% IF bad_relationships_count %]
 <p>Tietokantasi sisältää takaaja/taattava -pareja, joilla ei ole määriteltyä suhdetta. Niille on annettu arvo '_bad_data' sarakkeissa [% "borrowers.relationship" | $HtmlTags tag="strong" %] ja/tai [% "borrower_relationships.relationship" | $HtmlTags tag="strong" %]. Korjaa ne käsin tallentamalla suhteet tai pyydä järjestelmäsi ylläpitäjää korjaamaan arvot.</p>
 [% END # /IF bad_relationships_count %]

 [% IF wrong_relationships %]
 <p>Seuraavia arvoja käytetään takaaja/taattava-suhteena, mutta niitä ei ole olemassa 'borrowerRelationship' -järjestelmäasetuksessa:</p>
 <ul>
 [% FOR rel IN wrong_relationships %]
 <li>[% rel.0 | html %]</li>
 [% END %]
 </ul>
 <p>Jos suhde on haluttu, lisää se 'borrowerRelationship' -järjestelmäasetukseen, muutoin pyydä järjestelmän pääkäyttäjää korjaamaan tietokannan arvot [% "borrowers.relationship" | $HtmlTags tag="strong" %] ja/tai [% "borrower_relationships.relationship" | $HtmlTags tag="strong" %].</p>
 [% END # /IF wrong_relationships %]
 [% END # /IF warnRelationships %]

 [% IF log4perl_errors %]
 <h2>Lokitustoiminto ei toimi oikein</h2>
 [% FOR e IN log4perl_errors %]
 [% SWITCH e %]
 [% CASE 'missing_config_entry' %]<span>Konfiguraatiotiedostossa ei ole 'log4perl_conf' merkintää.</span>
 [% CASE 'cannot_read_config_file' %]<span>Log4perl konfiguraatiotiedostoa ei voida avata.</span>
 [% CASE 'logfile_not_writable' %]<span>Yksi konfiguraatiotiedostossa listattu lokitiedosto ei ole kirjoitettavissa.</span>
 [% CASE 'cannot_init_module' %]<span>Koha::Logger-osaa ei voi käynnistää oikein (tarkista lokitiedosto).</span>
 [% CASE %]<span>Tuntematon virhe '[% e | html %]'.</span>
 [% END %]
 [% END # /FOR e %]
 [% END # /IF log4perl_errors %]

 [% IF warnConnectBroker %]
 <h2>Mahdotonta ottaa yhteyttä viestin välittäjään</h2>
 Viestin välittäjään (RabbitMQ) yhdistämisessä on virhe, tarkista Kohan lokitiedot. <br/>
 Ehkä sitä ei ole asennettu ja määritelty oikein? <br/>
 Ota yhteyttä järjestelmän ylläpitäjään. [% END # /IF warnConnectBroker %] [% IF weasyprint_missing || warnPrefRequireChoosingExistingAuthority || warnPrefEasyAnalyticalRecords || warnPrefAnonymousPatronOPACPrivacy || warnPrefAnonymousPatronAnonSuggestions || warnPrefAnonymousPatronOPACPrivacy_PatronDoesNotExist || warnPrefAnonymousPatronAnonSuggestions_PatronDoesNotExist || warnPrefKohaAdminEmailAddress || warnPrefOpacHiddenItems || warnPrefPatronSelfRegistrationDefaultCategory || invalid_yesno.count || warnNoActiveCurrency || AutoSelfCheckPatronDoesNotHaveSelfCheckPerm || AutoSelfCheckPatronHasTooManyPerm || warnStatisticsFieldsError || warnNoTemplateCaching || warnILLConfiguration || warnXSLT || oauth2_missing_deps || bad_yaml_prefs || warnIssuingRules || config_bcrypt_settings_no_set || warnHiddenBiblionumbers.size || warnFastCataloging %] <h2>Järjestelmän asetuksista johtuvat varoitukset</h2>
 <table>
 <caption>Asetukset ja parametrit</caption>
 [% IF (warnFastCataloging) %]
 <tr>
 <th scope="row"><strong>Varoitus</strong> </th>
 <td>"Fast add" (FA) -kuvailupohjaa ei ole määritetty ylläpidossa -> MARC-kuvailupohjat. Se kytkee pois päältä 'pikakuvailu'-toiminnon kuvailussa ja Lainaus ja palautus -osiossa.</td>
 </tr>
 [% END %]
 [% IF (warnPrefRequireChoosingExistingAuthority) %]
 <tr>
 <th scope="row"><strong>Varoitus</strong> </th>
 <td>Järjestelmäasetus 'RequireChoosingExistingAuthority' ei ole käytössä, mutta asetuksen 'AutoCreateAuthorities' pitää olla sallittu. Muutoin kuvailijat voivat lisätä termejä, joita ei linkitetä auktoriteetteihin.</td>
 </tr>
 [% END %]
 [% IF (warnPrefEasyAnalyticalRecords) %]
 <tr>
 <th scope="row"><strong>Varoitus</strong> </th>
 <td>Järjestelmäasetus 'EasyAnalyticalRecords' on asetettu, mutta UseControlNumber -asetukseksi on valittu 'Käytä'. Valitse 'Älä käytä' tai 'Show analytics' -linkki virkailijaliittymässä ja verkkokirjastossa rikkoontuu.</td>
 </tr>
 [% END %]
 [% IF warnPrefAnonymousPatronOPACPrivacy %]
 <tr>
 <th scope="row"><strong class="error">Virhe</strong> </th>
 <td>Järjestelmäasetus 'OPACPrivacy' on asetettu, mutta asetus AnonymousPatron on '0'. Aseta siihen kelvollinen asiakkaan ID, muutoin palautukset epäonnistuvat.</td>
 </tr>
 [% END %]
 [% IF warnPrefAnonymousPatronAnonSuggestions %]
 <tr>
 <th scope="row"><strong>Varoitus</strong> </th>
 <td>Järjestelmäasetus 'AnonSuggestions' on asetettu, mutta järjestelmäasetukseen AnonymousPatron on asetettu arvoksi '0'. Aseta siihen kelvollinen asiakkaan ID, jos haluat tämän ominaisuuden toimivan oikein.</td>
 </tr>
 [% END %]
 [% IF warnPrefAnonymousPatronOPACPrivacy_PatronDoesNotExist %]
 <tr>
 <th scope="row"><strong class="error">Virhe</strong> </th>
