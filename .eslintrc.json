{
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-prettier"
    ],
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "prettier/prettier": [
            "error"
        ]
    }
}
